{
    "name": "ollywarren/prestashop-laravel",
    "description": "Provide a simple elegant wrapper around the Prestashop APi Library from Prestashop.",
    "keywords": ["Laravel", "Prestashop"],
    "homepage": "http://github.com/ollywarren/prestashop",
    "license": "MIT",
    "authors": [
        {
            "name": "Olly Warren",
            "email": "olly@ollywarren.com"
        }
    ],
    "require-dev":{},
    "require":{
        "phpunit/phpunit":"^7.0",
        "prestashop/prestashop-webservice-lib":"dev-master"
    },
    "autoload": {
        "psr-4": {
            "Ollywarren\\Prestashop\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ollywarren\\Prestashop\\PrestashopServiceProvider"
            ],

            "facades": [
                "Ollywarren\\Prestashop\\Facades\\Prestashop"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
